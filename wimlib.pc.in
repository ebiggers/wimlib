prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_PKGCONFIG_LIBDIR@
includedir=@CMAKE_PKGCONFIG_INCLUDEDIR@

Name: wimlib
Description: Library to extract, create, modify, and mount Windows Imaging (WIM) files
URL:  https://wimlib.net
Version: @PROJECT_VERSION@
Libs: -L${libdir} -lwim
Requires.private: @CMAKE_PKGCONFIG_PRIVATE_REQUIRES@
Libs.private: @CMAKE_PKGCONFIG_PRIVATE_LIBS@
Cflags: -I${includedir}
